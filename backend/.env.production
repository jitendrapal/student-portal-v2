# Production Environment Variables for Backend
NODE_ENV=production

# Server Configuration
PORT=5001

# Frontend URL (will be updated with actual Vercel URL)
FRONTEND_URL=https://your-app-name.vercel.app

# Database Configuration
DB_TYPE=file

# CORS Origins (will be updated with actual frontend URL)
CORS_ORIGINS=https://your-app-name.vercel.app,http://localhost:3000,http://localhost:5173,http://localhost:5174,http://localhost:5175,http://localhost:5176

# API Configuration
API_VERSION=v1
